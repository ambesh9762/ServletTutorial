<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>cookieManage</display-name>
		<servlet>
			<servlet-name>cookie</servlet-name>
			<servlet-class>com.ambesh.cookieServlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>cookie</servlet-name>
			<url-pattern>/cookieManage</url-pattern>
		</servlet-mapping>
		
		<servlet>
			<servlet-name>next</servlet-name>
			<servlet-class>com.ambesh.welcomeServlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>next</servlet-name>
			<url-pattern>/welcome</url-pattern>
		</servlet-mapping>
</web-app>